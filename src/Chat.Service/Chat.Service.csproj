        <PackageReference Include="FreeRedis" Version="1.1.5" />
        <PackageReference Include="Masa.Contrib.Authentication.Identity" Version="1.0.0-rc.4.4" />
        <PackageReference Include="Masa.Contrib.Data.EFCore.PostgreSql" Version="1.0.0-rc.4.4" />
        <PackageReference Include="Masa.Contrib.Data.Mapping.Mapster" Version="1.0.0-rc.4.4" />
        <PackageReference Include="Masa.Contrib.Data.UoW.EFCore" Version="1.0.0-rc.4.4" />
        <PackageReference Include="Masa.Contrib.Ddd.Domain" Version="1.0.0-rc.4.4" />
        <PackageReference Include="Masa.Contrib.Ddd.Domain.Repository.EFCore" Version="1.0.0-rc.4.4" />
        <PackageReference Include="Masa.Contrib.Dispatcher.Events" Version="1.0.0-rc.4.4" />
